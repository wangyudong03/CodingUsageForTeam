{
  "manifest_version": 3,
  "name": "Trae Usage Token Extractor",
  "version": "1.3.0",
  "description": "Automatically extract and copy session tokens from Cursor and Trae for seamless IDE integration.",
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "tabs",
    "cookies"
  ],
  "host_permissions": [
    "*://*.trae.ai/*",
    "*://*.cursor.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "AI IDE Usage Token Extractor",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.trae.ai/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.cursor.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icon16.png",
        "icon48.png",
        "icon128.png"
      ],
      "matches": [
        "*://*.trae.ai/*",
        "*://*.cursor.com/*"
      ]
    }
  ],
  "minimum_chrome_version": "88"
}